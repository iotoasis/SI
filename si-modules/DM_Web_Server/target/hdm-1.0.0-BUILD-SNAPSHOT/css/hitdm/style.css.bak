#wrapper{position:relative;}
.wrap_div{width:990px;position:relative;margin:0 auto;}/*width:990px;*/

#header{height:120px;background:url(../../images/hitdm/common/header_bg2.gif) 0 0 repeat-x; *zoom:1;}
#header:after{content:""; display:block;clear:both;}
.header_top{position:absolute;right:0; top:10px;}
.header_top *{vertical-align:middle;}
.header_top span{margin-right:10px;font-weight:bold;}
.header_top a{display:inline-block;}
.header_top .header_login_button{background:url(../../images/hitdm/common/header_leftbt_bg.gif) 0 0 no-repeat;width:67px;height:22px;line-height:22px;text-align:center;}
.header_top .header_join_button{background:url(../../images/hitdm/common/header_rightbt_bg.gif) 0 0 no-repeat;width:67px;height:22px;line-height:22px;text-align:center}
h1{position:absolute;left:0px; top:40px;}
#gnb{padding-top:42px;float:right;padding-bottom:7px;background:url(../../images/hitdm/common/gnbbg_bottom.gif) 0 bottom repeat-x;}
#gnb ul{*zoom:1}
#gnb ul:after{content:""; display:block;clear:both;}
#gnb ul li{float:left;position:relative;}
#gnb ul li a{display:block;width:132px;height:33px;border:1px solid #373737;line-height:33px;text-align:center;color:#fff;font-weight:bold;margin-left:2px;background:url(../../images/hitdm/common/gnbbg.gif) 0 0 repeat-x;font-size:14px;}
#gnb ul li.gnb_hover a, #gnb ul li.gnb_active a{background:url(../../images/hitdm/common/gnbbg_ov.gif) 0 0 repeat-x;border:1px solid #6a9c07;}
#gnb ul li.gnb_first a{margin-left:0;}

.gnb_sub{position:absolute;display:none;top:33px;z-index:99990;padding-top:3px;}
#gnb .gnb_sub li a{font-size:12px;color:#333;border:none;height:33px; line-height:33px;display:inline-block;padding:0 7px 0 11px; text-align:center;width:auto;border:none !important;background-image:url(../../images/hitdm/common/gnb_space.gif) !important;background-position:left 50% !important;background-repeat:no-repeat !important;}
#gnb .gnb_sub li.first a{background:none !important;}
#gnb .gnb_sub li a:hover{color:#690;}
#gnb .gnb_sub .gnb_active a{color:#690;}

.gnb_1{width:490px;left:0px;}
.gnb_2{width:370px;left:-100px;}
.gnb_3{width:150px;left:-5px;}
.gnb_4{width:680px;left:0px;}
.gnb_5{width:390px;left:-200px;}
#gnb .gnb_active .gnb_sub{display:block !important;}

