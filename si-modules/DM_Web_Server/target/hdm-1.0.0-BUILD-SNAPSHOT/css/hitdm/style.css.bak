ul.tabs:after{content:""; display:block;clear:both;}
ul.tabs li {
	float: left;
	margin-left:3px;
	padding: 0;
	height: 28px; /*--Subtract 1px from the height of the unordered list--*/
	line-height: 28px; /*--Vertically aligns the text within the tab--*/
	border: 1px solid #e2e2e2;
	border-bottom:1px solid #000;
	margin-bottom: -1px; /*--Pull the list item down 1px--*/
	overflow: hidden;
	position: relative;
	background:url(/images/hitdm/common/contents_tab_bg.gif) 0 -1px repeat-x;

}
ul.tabs li.last{margin-right:0;}
ul.tabs li a {
	text-decoration: none;
	color: #888;
	display: block;
	font-size: 12px;
	font-weight:bold;
	outline: none;
	width:240px;
	text-align:center;
}
ul.tabs li a:hover {
}
ul.tabs li.active  { /*--Makes sure that the active tab does not listen to the hover properties--*/
	border:1px solid #000;
	background: #fff;
	border-bottom: 1px solid #fff; /*--Makes the active tab look like it's connected with its content--*/
}
ul.tabs li.active a{color:#000;}

.tab_container {
	display:none;
	padding-bottom:100px;
}

.current_container {
	display: block;	
}


